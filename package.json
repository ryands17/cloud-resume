{
  "name": "cloud-resume",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "check": "tsc --noEmit",
    "preview": "astro preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@shoelace-style/shoelace": "^2.16.0",
    "astro": "^4.13.0",
    "mdast-util-to-string": "^4.0.0",
    "reading-time-estimator": "^1.11.0",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.12.3",
    "sharp": "^0.32.6",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts, astro, cjs, mjs, js, mdx, md, css}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.7.4+sha1.9dfcac89c4010a00ce092c3cda76dab72cd99198"
}
