{
  "name": "cloud-resume",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "sst bind astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "tsc --noEmit",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "sst:build": "sst build",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.2.0",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.5.5",
    "mdast-util-to-string": "^4.0.0",
    "reading-time-estimator": "^1.10.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "aws-cdk-lib": "2.101.1",
    "constructs": "10.2.69",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.12.3",
    "sharp": "^0.32.6",
    "sst": "^2.32.1",
    "typescript": "^5.4.2"
  },
  "prettier": {
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts, astro, cjs, mjs, js, mdx, md, css}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.7.4+sha1.9dfcac89c4010a00ce092c3cda76dab72cd99198"
}
